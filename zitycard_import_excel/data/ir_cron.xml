<odoo>
    <record id="ir_cron_import_files" model="ir.cron">
        <field name="name">Zitycard Importar Archivos</field>
        <field name="model_id" ref="model_import_file"/>
        <field name="state">code</field>
        <field name="code">model.import_files()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
    </record>
</odoo>
